  "compilerOptions": {
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "noImplicitReturns": true,
    "outDir": ".compiled",
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": ["./src/**/*.ts", "./src/**/*.tsx"],
  "exclude": ["./src/client/"]
}
